
<!-- /templates/page.liquid -->

<!-- ======================== Header ========================= -->

     
<div class="header_banner blog_banner">
    	<h1>{{ page.title }}</h1>
    	<span class="header_overlay"></span>
        {% if settings.page_banner != blank %} 	
        	<img src="{{ settings.page_banner | img_url : 'master' }}" alt="{{ page.title }}" />
        {% else %}
        {{ 'image' | placeholder_svg_tag: 'icon icon--placeholder' }}
       {% endif %} 
</div>   



<!-- ======================= About Content ==================== -->


<div class="container">
	{{ page.content }}
</div>

<div class="partnerships" id="partnerships" >
  <div class="partnership">
    {% section 'partnerships-header-template' %}
    {% section 'partnership-logos-template' %}
  </div>
</div>




