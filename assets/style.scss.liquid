      }
      
      @include at-query ($postMedium, $large) {
        padding: 20px 16px 0 14px;
      }
      
      h3{
        text-transform:uppercase;
        font-size: 28px;
        font-family: 'Oswald', sans-serif;
        margin-bottom: 25px;
        color: $colorGray;
        letter-spacing: 2px;
        
        @include at-query ($max, $medium) {
          font-size: 20px;
          text-align: left;
        }
        
        /* @include at-query ($large, $maxlarge) {
          font-size:16px;
        } */
        
        span{
          color:$colorSecondary;
          margin-left:20px;
          
          @include at-query ($max, $medium) {
            float: right;
          }
        }
      }
      
      .btn_row{
        a{
          margin-right:25px;
          //line-height: 20px;
          
          @include at-query ($max, $medium) {
            width: 100%;
            margin-bottom: 15px;
            text-align: center;
          }
          
          @include at-query ($postSmall, $medium) {
            width: 46%;
            margin-bottom: 0;
            margin-right: 16px;
          }
          
          /* @include at-query ($large, $maxlarge) {
            width: 100%;
            margin-bottom:15px;
            text-align: center;
          } */
        }
        
        button{
          
          @include at-query ($max, $small) {
            width:100%;
          }
          
          @include at-query ($postSmall, $medium) {
            width: 46%;
          }
          
          /* @include at-query ($large, $maxlarge) {
            padding: 10px 0;
            width: 100%;
          } */
        }
      }
    }
  }
  
  .related_product{
    @include at-query ($max, $medium) {
      margin-top:40px;
    }
    
    @include at-query ($postMedium, $large) {
      margin-top: 55px;
    }
    
    .product-carousel{
      
      .owl-controls{
        opacity:0;
        @include transition(0.4s all ease-in-out);
        
        @include at-query ($max, $large){
          opacity:1;
        }
        
        @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
          visibility: hidden;
        }
        
        .owl-prev{
          position: absolute;
          top: 50%;
          font-size:0;
          margin-top: -90px;
          left:-10px;
          @include transition(0.4s all ease-in-out);
          
          @include at-query ($max, $large){
            left:10px;
          }
          
          &:after{
            content: "\f3d5";
            display:block;
            font-size: 53px;
            font-family:"Ionicons";
          }
        }
        .owl-next{
          left:auto;
          @extend .owl-prev;
          right:-10px;
          @include transition(0.4s all ease-in-out);
          
          @include at-query ($max, $large){
            right:10px;
          }
          
          &:after{
            content: "\f3d6";
          }
        }
      }
      
      &:hover{
        .owl-controls{
          opacity:1;
          
          @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
            visibility: visible;
          }
          
          .owl-prev{
            left:10px;
          }
          .owl-next{
            right:10px;
            left:auto;
          }
        }
      }
    }
    
    h1{
      font-size:30px;
    }
  }
  
  /* ================== Empty Cart ===================== */
  
  .empty_cart{
    text-align: center;
    padding: 65px 0;
    
    span{
      font-size: 130px;
      color: $colorGray;
      
      @include at-query ($max, $medium) {
        font-size: 80px;
      }
    }
    
    @include at-query ($max, $medium) {
      padding: 0;
    }
    
    @include at-query ($postMedium, $large) {
      padding-top: 0;
      padding-bottom: 10px;
    }
  }
  
  a.btn{
    @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
      line-height: 20px;
    }
  }
}


