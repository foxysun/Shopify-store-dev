@include at-query ($max, $medium) {
  .content .product_detail_section {
    padding-top:10px;
  }
  
  .breadcrumb {
    padding: 0 10px;
    margin-bottom: 5px;
    
    a, span {
      padding: 0 3px 0 0;
      margin-right: 3px;
      font-size: 10px;
      letter-spacing: 0.5px;
    }
  }
  
  .product_carousel {
    width: 70%;
    margin: auto;
  }
  
  .product_detail > h2,
  .template-product .grid__item.two-tenths,
    #mainImage,
    .mf_item{
      display: none;
    }
  
  .product_detail {
  	.price,
    .product_variant {
      position: relative;
      top: 50px;
    }
    
    .product_variant {
      top: -50px
    }
  }
  
  .btn, .btn--secondary, .rte .btn--secondary, .rte .btn, .rte .btn--secondary {
    line-height: 50px;
    height: 50px;
  }
  
  .mob-only,
  .mf_item.mf_prev,
  .mf_item.mf_stage,
  .mf_item.mf_next {
    display: block;
  }

  h2.mob-only {
    text-transform: uppercase;
    color: #747474;
    letter-spacing: 0;
    font-size: 22px;
  }
  
  .mf_prev,
  .mf_next {
    cursor: pointer;
    transition: all 300ms;
  }
  
  .mf_prev,
  .mf_next,
  .mf_stage {
    position: absolute;
    bottom: 0;
    //transition: all 100ms;
  }
  
  .mf_prev {
    //background: #fc0;
    width: 18%;
    left: 0;
    right: 82%;
    z-index: 40;
  }

  .mf_stage {
    //background: #f0c;
    width: 60%;
    margin: auto;
    left: 0;
    right: 0;
    //height: 400px;
    //transition: left 500ms;
    transition: all 300ms;
    z-index: 50;
  }
  .mf_next {
    //background: #cf0;
    width: 18%;
    right: 0;
    left: 82%;
  }

  #mobFlick {
    overflow: hidden;
    height: 400px;
    position: relative;
    
    &.rightward {
      .mf_prev {
        z-index: 20;
  	  }
      .mf_stage {
        // transition: right 600ms;
      }
      .mf_next {
        z-index: 70;
      }
	}
    
    &.leftward {
      .mf_prev {
        z-index: 70;
  	  }
      .mf_stage {
        // transition: right 600ms;
      }
      .mf_next {
        z-index: 20;
      }
	}
  }
}

