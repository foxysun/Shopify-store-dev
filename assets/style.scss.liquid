        @include at-query ($max, $medium) {
          width: 45px;
          line-height: 25px;
        }
      }
      
      button{
        position: absolute;
        top: 0;
        font-size: 25px;
        color: $colorGray;
        @include transition(0.3s all ease-in-out);
        
        &:hover{
          color: $colorSecondary;
        }
        
        &.minus{
          left: 12px;
        }
        &.plus{
          right: 12px;
        }
      }
    }
    
    .cart-total{
      @extend .cart-price;
    }
    
    .cart-cancel{
      @extend .cart-price;
      text-align:center;
      display:inline-block;
      width:17px;
      height:17px;
      
      @include at-query ($max, $small) {
        position: absolute !important;
        bottom: 25px;
        right: 0;
        
      }
      
