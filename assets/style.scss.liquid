/*============================================================================
  #Breakpoint and Grid Variables
==============================================================================*/
$viewportIncrement: 1px;

$small: 480px;
$medium: 767px;
$maxMedium: 991px;
$large: 1024px;
$maxlarge : 1368px;

$postSmall: $small + $viewportIncrement;
$preMedium: $medium - $viewportIncrement;
$postMedium: $medium + $viewportIncrement;
$preLarge: $large - $viewportIncrement;
$postLarge: $large + $viewportIncrement;

/*================ The following are dependencies of csswizardry grid ================*/
$breakpoints: (
  'small' '(max-width: #{$small})',
  'medium' '(min-width: #{$postSmall}) and (max-width: #{$medium})',
  'medium-down' '(max-width: #{$medium})',
  'medium-large' '(min-width: #{$medium}) and (max-width: #{$large})',
  'large' '(min-width: #{$large})',
  'maxlarge' '(min-width: #{$postLarge}) and (max-width: #{$maxlarge})'
);
$breakpoint-has-widths: ('small', 'medium', 'medium-down', 'large', 'medium-large', 'maxlarge', 'medium-max');
$breakpoint-has-push:  ('medium', 'medium-down', 'large', 'medium-large', 'maxlarge', 'medium-max');
$breakpoint-has-pull:  ('medium', 'medium-down', 'large', 'medium-large', 'maxlarge', 'medium-max');

